{
  "name": "youtubex-studio-clone",
  "version": "2.0.0",
  "description": "A modern YoutubeX Studio clone built with React, TypeScript, and Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "dev:api": "node server/dev-server.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:client-only": "vite",
    "dev:debug": "vite --debug",
    "dev:performance": "VITE_PERFORMANCE_OVERLAY=true vite",
    "build": "vite build",
    "build:with-type-check": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "preview": "vite preview",
    "preview:production": "vite preview --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui --config ./vitest.config.ts",
    "test:coverage": "vitest --coverage",
    "test:coverage:open": "vitest --coverage && open coverage/index.html",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:components": "vitest run components",
    "test:hooks": "vitest run hooks",
    "test:services": "vitest run services",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse-ci autorun",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --quiet",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:strict": "eslint . --ext ts,tsx --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rimraf dist coverage .nyc_output",
    "clean:cache": "rimraf node_modules/.cache .vite",
    "clean:all": "npm run clean && npm run clean:cache && rimraf node_modules",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist/stats.html",
    "analyze:size": "npx bundlesize",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "validate": "npm run type-check && npm run lint:strict && npm run test:run",
    "validate:ci": "npm run type-check && npm run lint:strict && npm run test:coverage",
    "prepare": "husky install",
    "postinstall": "npm run type-check",
    "start": "npm run dev",
    "start:production": "npm run build && npm run preview:production",
    "refactor": "node scripts/refactor-optimize.js",
    "refactor:full": "./scripts/refactor.sh",
    "refactor:analyze": "node scripts/refactor-optimize.js --analyze-only",
    "refactor:comprehensive": "node scripts/comprehensive-refactor-v2.js",
    "refactor:iterative": "node scripts/iterative-optimizer.js",
    "test:memory-safe": "NODE_OPTIONS=\"--max-old-space-size=4096\" vitest run --pool=forks --poolOptions.forks.singleFork=true",
    "build:optimized": "NODE_OPTIONS=\"--max-old-space-size=8192\" npm run build",
    "validate:comprehensive": "npm run type-check && npm run lint && npm run test:memory-safe",
    "validate:all": "./scripts/validate-all.sh",
    "optimize": "npm run refactor:iterative",
    "test:safe": "NODE_OPTIONS=\"--max-old-space-size=8192\" vitest run --pool=threads --poolOptions.threads.singleThread=true --reporter=basic",
    "test:watch:safe": "NODE_OPTIONS=\"--max-old-space-size=8192\" vitest --pool=threads --poolOptions.threads.singleThread=true",
    "test:simple": "NODE_OPTIONS=\"--max-old-space-size=8192\" vitest run --pool=forks --poolOptions.forks.singleFork=true --reporter=basic --no-coverage",
    "validate:quick": "npm run type-check && npm run lint",
    "validate:full": "npm run type-check && npm run lint && npm run build:optimized",
    "count-errors": "node scripts/count-errors.js",
    "fix:ts2304": "node scripts/fix-ts2304-cannot-find-name.js",
    "fix:ts2300": "node scripts/fix-ts2300-duplicate-identifier.js",
    "fix:ts2322": "node scripts/fix-ts2322-type-not-assignable.js",
    "fix:ts6133": "node scripts/fix-ts6133-declared-not-used.js",
    "fix:ts18048": "node scripts/fix-ts18048-possibly-undefined.js",
    "fix:ts7019": "node scripts/fix-ts7019-implicit-any.js",
    "fix:all-types": "npm run fix:ts2304 && npm run fix:ts2300 && npm run fix:ts2322 && npm run fix:ts6133 && npm run fix:ts18048 && npm run fix:ts7019",
    "fix:all": "node scripts/fix-all-errors.js"
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "@google/generative-ai": "^0.2.1",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-query": "^5.17.0",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^6.20.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "tailwind-merge": "^2.6.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.57",
    "@types/react": "^18.3.23",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "msw": "^2.8.7",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.3.6",
    "terser": "^5.40.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "youtubex",
    "studio",
    "clone",
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "zustand",
    "react-query"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtubex-studio-clone.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/youtubex-studio-clone/issues"
  },
  "homepage": "https://github.com/yourusername/youtubex-studio-clone#readme"
}
