{
  "name": "typescript-error-resolution",
  "version": "1.0.0",
  "description": "Automated TypeScript error resolution system with intelligent categorization and bulk fixing capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "error-resolver": "dist/cli/main.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/main.js",
    "dev": "ts-node src/error-resolution/cli/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "error-resolution",
    "automation",
    "code-fixing",
    "compilation-errors",
    "developer-tools"
  ],
  "author": "TypeScript Error Resolution Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/typescript-error-resolution.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/typescript-error-resolution/issues"
  },
  "homepage": "https://github.com/your-org/typescript-error-resolution#readme"
}
